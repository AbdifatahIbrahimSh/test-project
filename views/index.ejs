<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Data from Neon Database</title>
</head>
<body>
    <h1>Data from Neon Database</h1>
    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Email</th>
                <th>Title</th>
                <!-- Add other columns as necessary -->
            </tr>
        </thead>
        <tbody>
            <% data.forEach(course => { %>
                <tr>
                    <td><%= course.id %></td>
                    <td><%= course.instructor %></td>
                    <td><%= course.price %></td>
                    <td><%= course.title %></td>
                    <!-- Add other columns as necessary -->
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>